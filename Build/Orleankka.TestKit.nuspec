<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Orleankka.TestKit</id>
    <version>$version$</version>
    <authors>Yevhen Bobrov</authors>
    <owners>Yevhen Bobrov</owners>
    <licenseUrl>http://github.com/yevhen/Orleankka/blob/master/LICENSE</licenseUrl>
    <projectUrl>http://github.com/yevhen/Orleankka</projectUrl>
    <iconUrl>https://github.com/OrleansContrib/Orleankka/raw/master/Logo.png</iconUrl>    
    <description>Unit testing kit for Orleankka. Allows to unit test actors in a full isolation without the need to spawn full blown integration silo.</description>
    <tags>Orleans Akka distributed messaging actor concurrency CQRS unit testing mock</tags>
    <dependencies>
        <group targetFramework=".NETStandard2.0">
            <dependency id="NETStandard.Library" version="2.0.0" exclude="Build,Analyzers" />
            <dependency id="Orleankka.Runtime" version="$version$" />
        </group>
    </dependencies>  
  </metadata>
  <files>
    <file src="Output\Package\Release\Orleankka.TestKit.dll" target="lib/netstandard2.0" />
    <file src="Output\Package\Release\Orleankka.TestKit.pdb" target="lib/netstandard2.0" />
    <file src="Output\Package\Release\Orleankka.TestKit.xml" target="lib/netstandard2.0" />
  </files>
</package>